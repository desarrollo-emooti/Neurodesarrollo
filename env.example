# EMOOTI Frontend Environment Configuration
# ========================================

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api/v1
VITE_API_TIMEOUT=10000

# Authentication
VITE_JWT_STORAGE_KEY=emooti_token
VITE_REFRESH_TOKEN_KEY=emooti_refresh_token

# Google OAuth (if using)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Application Configuration
VITE_APP_NAME=EMOOTI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_DATA=false

# External Services
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
VITE_QR_CODE_API_URL=https://api.qrserver.com/v1/create-qr-code

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf,text/plain

# Map Configuration (if using)
VITE_MAP_TILE_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_ATTRIBUTION=Â© OpenStreetMap contributors

# Development Tools
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_REACT_QUERY_DEVTOOLS=true